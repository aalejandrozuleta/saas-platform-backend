{
  "title": "SaaS Platform â€“ Overview",
  "uid": "saas-overview",
  "schemaVersion": 38,
  "version": 3,
  "refresh": "5s",
  "templating": {
    "list": [
      {
        "name": "service",
        "type": "query",
        "datasource": "Prometheus",
        "refresh": 1,
        "query": "label_values(http_requests_total, service)",
        "multi": false,
        "includeAll": false
      }
    ]
  },
  "panels": [
    {
      "type": "stat",
      "title": "Requests / sec",
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{service=\"$service\",route!=\"/metrics\"}[2m]))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Error Rate %",
      "gridPos": { "x": 6, "y": 0, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "(sum(rate(http_requests_total{service=\"$service\",status=~\"5..\"}[2m])) / sum(rate(http_requests_total{service=\"$service\"}[2m]))) or vector(0)",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "P95 Latency (ms)",
      "gridPos": { "x": 12, "y": 0, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{service=\"$service\",route!=\"/metrics\"}[5m])) by (le)) * 1000",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Memory (MB)",
      "gridPos": { "x": 18, "y": 0, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "process_resident_memory_bytes{service=\"$service\"} / 1024 / 1024",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Requests by Route",
      "gridPos": { "x": 0, "y": 4, "w": 12, "h": 6 },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{service=\"$service\",route!=\"/metrics\"}[2m])) by (route)",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Latency by Route (P95)",
      "gridPos": { "x": 12, "y": 4, "w": 12, "h": 6 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{service=\"$service\",route!=\"/metrics\"}[5m])) by (le, route)) * 1000",
          "refId": "A"
        }
      ]
    }
  ]
}