{
  "title": "SaaS Platform â€“ Overview",
  "uid": "saas-overview",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "type": "stat",
      "title": "Requests / sec",
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[1m]))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "P95 Latency (ms)",
      "gridPos": { "x": 6, "y": 0, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le)) * 1000",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Memory (MB)",
      "gridPos": { "x": 12, "y": 0, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "process_resident_memory_bytes / 1024 / 1024",
          "refId": "A"
        }
      ]
    },
    {
      "type": "graph",
      "title": "HTTP Requests by Service",
      "gridPos": { "x": 0, "y": 4, "w": 18, "h": 6 },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[1m])) by (service)",
          "refId": "A"
        }
      ]
    }
  ]
}
